pipeline{
agent any
stages{
stage('Build'){
steps{
echo 'Build started......................'
sh 'mvn clean install -DskipTests'
}
}
stage('Test'){
steps{
echo 'Testing.........................'
sh 'mvn test'
}
post{
always{
junit 'target/surefire-reports/*.xml'
archieveArtifacts 'target/*.jar'
}
}
}
stage('Run'){
steps{
echo 'Running.....................'
}
}


}




}
